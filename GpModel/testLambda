from sympy import symbols, lambdify, parse_expr, count_ops, preorder_traversal, Function, Add, Pow, Mod, Symbol, simplify, expand

# expression = "((((x1+x9)+41.432000)/ ((x10/ (x2+1e-6) )+1e-6) )+(x8+x8))"
ex = ("x0+x1+x9")
expr = parse_expr(ex, evaluate=False)
expr = Add("x0+x1", "x9")
print(expr)
print(simplify(expr))